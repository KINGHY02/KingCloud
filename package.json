{
  "name": "kingcloud",
  "version": "1.0.0",
  "description": "KingCloud代理",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.kingcloud.app",
    "productName": "KingCloud",
    "icon": "icons/512.png",
    "extraResources": [
      "clash.meta/**/*",
      "Xray/**/*",
      "hysteria/**/*",
      "hysteria2/**/*",
      "juicity/**/*",
      "mieru/**/*",
      "naiveproxy/**/*",
      "shadowquic/**/*",
      "singbox/**/*",
      "wget.exe",
      "chrome-user-data/**/*",
      "Browser/**/*",
      "*.cmd",
      "*.vbs",
      "*.ps1"
    ],
    "win": {
      "target": ["nsis", "portable"]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "KingCloud",
      "runAfterFinish": true
    }
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "jquery": "^3.7.0"
  },
  "author": "KINGHY02"
}